<script>
	var current_image_page = 0;
	get_imagebox_content = function(page){
		
		$.post(Drupal.settings.basePath+"load_imagebox",{
			'page' : page
		},function(data){
	
			
			$(".kepgaleria_box").html(data);
			Lightbox.initList();
			
			$(".prev_video_page , .next_video_page").click(function(event){
				event.preventDefault();
				if($(this).attr("class") == 'prev_video_page')
					current_image_page -= 1;
				else
					current_image_page += 1;
				get_imagebox_content( current_image_page );	
			})


		})
	}


  	 function refreshMidPos(){	
  	 	if( $(window).height() > $("#container").height() -20 )
  	 		$(".sulyzok_footer").addClass("fullbottom");
  	 	else
  	 		$(".sulyzok_footer").removeClass("fullbottom");
  	 	
  	 }
  	 


	$(document).ready(function(){
		
		get_imagebox_content( current_image_page );
		
		refreshMidPos();
		$(window).resize(function(){
			refreshMidPos();
		});
		

	})
</script>    
  
